                    <li class="nav-item">
                        <a class="nav-link " href="/contact">Contact</a>
                    </li>
                    <% if(user){ %>
                        <li class="nav-item dropdown">
                     <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                       <%= user.name %>
                     </a>
                     <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                        <li> <a class="dropdown-item" href="/profile"><i class="fa fa-user-circle" aria-hidden="true"></i>&nbsp; My Profile</a></li>
                        <li><a class="dropdown-item" href="/customers/orders"><i class="fa fa-first-order" aria-hidden="true"></i></i>&nbsp; Your Orders</a></li>
                        <li><a class="dropdown-item" href="/logout"><i class="fa fa-sign-out" aria-hidden="true"></i>&nbsp; Logout</a></li>
                      
                     </ul>
                   </li>
                   <% } else {%>
                   <li class="nav-item">
                     <a class="nav-link" href="/login">Login</a>
                   </li>
                   <% } %>
                    <li class="nav-item">
                        <a class="nav-link cart" href="/cart"><i class="fas fa-shopping-cart"></i></a>
                    </li>
                </ul>

            </div>
            <form class="d-flex">
                <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
                <button class="btn btn-sm btn-primary" type="submit">Search</button>
            </form>
        </div>
    </nav>

    <div class="login">

        <div class="container">
            <div class="row">
                <div class="col-sm loginpage">
                    <h1 class="text-center loginh1">
                        Reset Password
                    </h1>
                    <%- include('messages') %>
                    <form action="/reset/<%= id %>" method="POST">
                        <div class="mb-3">
                            <label for="exampleInputPassword1" class="form-label">Password</label>
                            <input type="password" name="password" class="form-control" id="exampleInputPassword1">
                        </div>

                        <div class="mb-3">
                            <label for="exampleInputPassword1" class="form-label">Confirm Password</label>
                            <input type="password" name="confpassword" class="form-control" id="exampleInputPassword1">
                        </div>

                        <button type="submit" class="btn btn-primary loginsub">Submit</button>
                    </form>

                    <p class="regtxt"> <a href="/login">Login</a> </p>

                    <p class="regtxt register-lg"> <a href="/register"> Register</a> </p>
                    
                    <p class="regtxt register-text" >Register with E-Learning</p>

                </div>
                <div class="col-sm">
                    <img class="img-fluid loginimg" src="/images/elearningpic.jpeg" alt="">




                </div>
            </div>
        </div>

    </div>
   

   



  



